# Environment variables for HomelabMapper
# Copy this file to .env and fill in your credentials

# Proxmox API Token (format: user@realm!tokenid=secret)
PROXMOX_TOKEN=your_proxmox_token_here

# Portainer API Token
PORTAINER_TOKEN=your_portainer_token_here

# Docker API Token (if needed)
DOCKER_TOKEN=your_docker_token_here

# Unraid API Key (if needed)
UNRAID_API_KEY=your_unraid_api_key_here

# Router Password (for ZTE F670Y or similar)
ROUTER_PASSWORD=your_router_password_here

# SSH Password (if using SSH for scanning)
SSH_PASSWORD=your_ssh_password_here

# Timezone (default: UTC)
TZ=UTC
